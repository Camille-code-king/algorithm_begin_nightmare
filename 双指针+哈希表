class Solution {
public:
    int maxSubarrayLength(vector<int>& nums, int k) {
        int n=nums.size();
        map<int,int>memo;
        int l=0;
        int ans=0;
        for(int r=0;r<n;r++){
            memo[nums[r]]++;
            while(memo[nums[r]]>k)memo[nums[l++]]--;
            ans=max(ans,r-l+1);
        }
        return ans;
    }
};
