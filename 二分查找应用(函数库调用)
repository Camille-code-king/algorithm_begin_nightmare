using namespace std;
#include<iostream>
#include<vector>
#include<algorithm>
class Solution {
public:
    vector<int> successfulPairs(vector<int>& spells, vector<int>& potions, long long success) {
        int sn=spells.size();
        int n=potions.size();
        vector<int>ans(sn,0);
        sort(potions.begin(),potions.end());
        for(int i=0;i<sn;i++)
        ans[i]=n-(lower_bound(potions.begin(),potions.end(),(success+spells[i]-1)/spells[i])-potions.begin());
        return ans;
    }
};
