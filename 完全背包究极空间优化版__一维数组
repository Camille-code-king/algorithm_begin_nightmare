class Solution {
public:
    int change(int amount, vector<int>& coins) {
        int n=coins.size();
        vector<unsigned>f(amount+1,0);
        f[0]=1;
        for(int i=0;i<n;i++){
            for(int c=0;c<=amount;c++){
                if(c>=coins[i])
                f[c]+=f[c-coins[i]];
            }
        }
        return f[amount];
    }
};
